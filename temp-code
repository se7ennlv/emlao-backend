/* eslint-disable prettier/prettier */
import { IsNotEmpty, IsNotEmptyObject, IsEmail } from 'class-validator';

export class Role {
    @IsNotEmpty()
    code: string;

    @IsNotEmpty()
    name: string;
}

export class CreateAccountDto {
    @IsNotEmpty()
    @IsEmail()
    username: string;

    @IsNotEmpty()
    password: string;

    @IsNotEmptyObject()
    role: Role;
}
